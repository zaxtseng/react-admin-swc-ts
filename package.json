		"cz-git": "^1.6.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.4",
		"release-it": "^15.8.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"typescript": "^4.9.3",
		"vite": "^4.1.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
